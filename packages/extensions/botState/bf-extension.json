{
  "name": "Bot State Visualizer",
  "client": {
    "inspectors": [
      {
        "name": "Bot State Visualizer",
        "src": "public/index.html",
        "criteria": [
          {
            "path": "valueType",
            "value": "https://www.botframework.com/schemas/botState"
          },
          {
            "path": "valueType",
            "value": "https://www.botframework.com/schemas/diff"
          }
        ],
        "summaryText": [
          "label"
        ],
        "accessories": [
          {
            "id": "graph",
            "states": {
              "default": {
                "label": "STATE GRAPH"
              },
              "selected": {
                "label": "STATE GRAPH",
                "aria-selected": true
              }
            }
          },
          {
            "id": "json",
            "states": {
              "default": {
                "label": "STATE JSON"
              },
              "selected": {
                "label": "STATE JSON",
                "aria-selected": true
              }
            }
          }
        ]
      }
    ]
  }
}
